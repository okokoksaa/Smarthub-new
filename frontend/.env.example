# CDF Smart Hub Frontend Environment Variables
# Copy this file to .env and fill in the values

# ===========================================
# SUPABASE CONFIGURATION (Required)
# ===========================================
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your-supabase-anon-key
VITE_SUPABASE_PROJECT_ID=your-project-id

# ===========================================
# API GATEWAY (Required)
# ===========================================
# Development: http://localhost:3000/api/v1
# Production: https://api.yourdomain.com/api/v1
VITE_API_GATEWAY_URL=http://localhost:3000/api/v1

# ===========================================
# AI SERVICE (Optional)
# ===========================================
# Development: http://localhost:3002/api/v1
# Production: https://ai.yourdomain.com/api/v1
VITE_AI_SERVICE_URL=http://localhost:3002/api/v1

# ===========================================
# FEATURE FLAGS (Optional)
# ===========================================
VITE_ENABLE_AI_CHAT=true
VITE_ENABLE_PUBLIC_PORTAL=true
VITE_ENABLE_ANALYTICS=false

# ===========================================
# MONITORING (Optional - Production)
# ===========================================
VITE_SENTRY_DSN=
VITE_ANALYTICS_ID=
